{
  "editor.renderWhitespace": "all",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true,
    "editor.rulers": [120],
    "editor.renderWhitespace": "all",
  },
  "cSpell.words": [
    "canonicalize",
    "fenv",
    "formatdoc",
    "getppid",
    "Heejoon",
    "indoc",
    "Kang",
    "norc",
    "powdream",
    "PPID",
    "precache",
    "printdoc",
    "pyenv",
    "replacen",
    "tempdir",
    "tempfile",
    "unistd",
    "writedoc"
  ],
  "cSpell.ignorePaths": [
    ".vscode",
    "Cargo.*",
    "rust.yml",
    "**/.rustup/**",
    "**/.cargo/**"
  ],
  "rust-analyzer.showUnlinkedFileNotification": false,
  "files.trimTrailingWhitespace": true,
  "livePreview.defaultPreviewPath": "/target/coverage/html/index.html",
  "coverage-gutters.coverageFileNames": [
    "target/coverage/tests.lcov",
    "lcov.info",
    "cov.xml",
    "coverage.xml",
    "jacoco.xml",
    "coverage.cobertura.xml"
  ],

}
