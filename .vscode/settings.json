{
  "editor.renderWhitespace": "all",
  "editor.formatOnSave": true,
  "editor.rulers": [80, 100, 120],
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.indentSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "cSpell.words": [
    "aarch",
    "autoload",
    "automock",
    "canonicalize",
    "chrono",
    "codecov",
    "compdef",
    "compinit",
    "consts",
    "dartlang",
    "fenv",
    "formatdoc",
    "getppid",
    "Heejoon",
    "indicatif",
    "indoc",
    "intellij",
    "Kang",
    "mockall",
    "norc",
    "outpath",
    "powdream",
    "PPID",
    "precache",
    "printdoc",
    "pubspec",
    "pyenv",
    "replacen",
    "reqwest",
    "tempdir",
    "tempfile",
    "unistd",
    "untar",
    "writedoc"
  ],
  "cSpell.ignorePaths": [
    ".vscode",
    "Cargo.*",
    "rust.yml",
    "**/.rustup/**",
    "**/.cargo/**"
  ],
  "rust-analyzer.showUnlinkedFileNotification": false,
  "files.trimTrailingWhitespace": true,
  "livePreview.defaultPreviewPath": "/target/coverage/html/index.html",
  "coverage-gutters.coverageFileNames": [
    "target/coverage/tests.lcov",
    "lcov.info",
    "cov.xml",
    "coverage.xml",
    "jacoco.xml",
    "coverage.cobertura.xml"
  ],
  "deno.enable": true,
  "deno.lint": true,
  "deno.unstable": false
}
